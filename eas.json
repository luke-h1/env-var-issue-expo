{
  "cli": {
    "version": ">= 16.20.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_CAKE_API_URL": "$CAKE_API_URL_PRODUCTION",
        "EXPO_PUBLIC_CAKE_API_URL_STAGING": "$CAKE_API_URL_STAGING"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_CAKE_API_URL": "$CAKE_API_URL_PRODUCTION",
        "EXPO_PUBLIC_CAKE_API_URL_STAGING": "$CAKE_API_URL_STAGING"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_CAKE_API_URL": "$CAKE_API_URL_PRODUCTION",
        "EXPO_PUBLIC_CAKE_API_URL_STAGING": "$CAKE_API_URL_STAGING"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6754607976"
      }
    }
  }
}
